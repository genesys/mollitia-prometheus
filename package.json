{
  "name": "@mollitia/prometheus",
  "version": "0.0.1",
  "description": "Prometheus Mollitia Plugin",
  "main": "dist/mollitia-prometheus.umd.js",
  "module": "dist/mollitia-prometheus.es5.js",
  "typings": "dist/types/mollitia-prometheus.d.ts",
  "files": [
    "dist"
  ],
  "author": "Jérémie Pichon <jeremie.pichon@genesys.com>",
  "scripts": {
    "dev": "run-p dev:lib dev:docs",
    "dev:lib": "npm run build:lib:tsc && run-p dev:lib:tsc dev:lib:rollup",
    "dev:lib:tsc": "tsc --watch --module commonjs",
    "dev:lib:rollup": "rollup -c rollup.config.ts -w",
    "dev:docs": "npm run build:docs:vue && run-p dev:docs:vue dev:docs:madoc",
    "dev:docs:vue": "vue-cli-service build --watch --target lib --name Playground docs/vue/entry.js",
    "dev:docs:madoc": "madoc dev",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --forceExit --detectOpenHandles",
    "test:prometheus:start": "cd devops && sh start.sh",
    "test:prometheus:stop": "cd devops && sh stop.sh",
    "build": "run-s build:lib build:docs",
    "build:lib": "rimraf dist && run-s build:lib:tsc build:lib:rollup",
    "build:lib:tsc": "tsc --module commonjs",
    "build:lib:rollup": "rollup -c rollup.config.ts",
    "build:docs": "run-s build:docs:vue build:docs:madoc",
    "build:docs:vue": "vue-cli-service build --target lib --name Playground docs/vue/entry.js",
    "build:docs:madoc": "madoc build",
    "serve": "madoc serve"
  },
  "devDependencies": {
    "@madoc/core": "0.0.17",
    "@types/jest": "26.0.14",
    "@types/node": "14.10.3",
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "@vue/cli-service": "4.5.6",
    "chart.js": "2.9.3",
    "eslint": "7.9.0",
    "jest": "26.4.2",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "prom-client": "12.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.27.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.6.2",
    "rollup-plugin-typescript2": "0.27.2",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2",
    "vue": "2.6.12",
    "vue-chartjs": "3.5.1",
    "vue-template-compiler": "2.6.12"
  }
}
